<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Events" table="events">
    <id name="idevent" type="integer" column="idEvent">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="datedebut" type="date" column="dateDebut" nullable="false"/>
    <field name="datefin" type="date" column="dateFin" nullable="false"/>
    <field name="lieu" type="string" column="lieu" length="255" nullable="false"/>
    <field name="nomevent" type="string" column="nomEvent" length="255" nullable="false"/>
    <field name="description" type="text" column="description" length="65535" nullable="false"/>
  </entity>
</doctrine-mapping>
