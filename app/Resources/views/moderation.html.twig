{% extends 'layout.html.twig' %}
{% block title %}E4{% endblock %}
{% block body %}
<body class="bodyFormulaire">
<div class="container">
    <div class="col-lg-12 goldenZoneFormulaire">
        <h1 id="AddMemberTitle">Demande a l'administrateur</h1>

        <form action="#" method="post" {{ form_enctype(form) }}>
            {{ form_row(form.mail) }}
            {{ form_row(form.request) }}
            {{ form_widget(form) }}
            {{ form_rest(form) }}
            <input type="submit" value="Valider" />
        </form>

    </div>
</div>
</body>
{% endblock %}