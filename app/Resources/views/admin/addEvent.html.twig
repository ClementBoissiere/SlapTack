{% extends 'layout.html.twig' %}
{% block title %}E4{% endblock %}
{% block body %}
    <body class="bodyFormulaire">
    <div class="container">
        <div class="col-lg-12 goldenZoneFormulaire">
            <h1 id="AddMemberTitle">Ajouter un Evenement</h1>

            <form action="#" method="post" {{ form_enctype(form) }}>
                {{ form_row(form.datedebut) }}
                {{ form_row(form.datefin) }}
                {{ form_row(form.lieu) }}
                {{ form_row(form.nomevent) }}
                {{ form_row(form.description) }}
                {{ form_widget(form) }}

                <input type="submit" value="Valider" />
            </form>
        </div>
    </div>
    </body>
{% endblock %}